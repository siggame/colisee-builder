{
  "name": "@siggame/colisee-builder",
  "version": "1.1.0",
  "description": "Microservice for building client code",
  "main": "release",
  "scripts": {
    "start": "node src",
    "start:prod": "NODE_ENV=production node release",
    "start:dev": "npm run build && node --inspect --trace-warnings src",
    "setup": "npm install",
    "test": "npm run build && mocha test",
    "build": "tsc",
    "build:dist": "tsc -p tsconfig.dist.json",
    "build:watch": "tsc --watch",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:misc",
    "clean:src": "rimraf src/**/*.js src/*.js",
    "clean:test": "rimraf test/**/*.js test/*.js",
    "clean:misc": "rimraf release/ node_modules/",
    "lint": "tslint --format verbose --project ./tsconfig.json",
    "lint-fix": "tslint --fix --format verbose --project ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/colisee-builder.git"
  },
  "author": "siggame@mst.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/colisee-builder/issues"
  },
  "homepage": "https://github.com/siggame/colisee-builder#readme",
  "devDependencies": {
    "@siggame/colisee-tslint": "^1.0.4",
    "@types/adm-zip": "^0.4.31",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/cors": "^2.8.3",
    "@types/dockerode": "^2.5.4",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/file-type": "^5.2.1",
    "@types/http-errors": "^1.6.1",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/multer": "^1.3.6",
    "@types/node": "^9.4.6",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@siggame/colisee-lib": "^0.3.1",
    "adm-zip": "^0.4.7",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dockerode": "^2.5.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "file-type": "^6.2.0",
    "http-errors": "^1.6.2",
    "lodash": "^4.17.5",
    "multer": "^1.3.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "tar-stream": "^1.5.5",
    "tstl": "^1.7.3",
    "winston": "^2.4.0"
  }
}
