{
  "name": "@siggame/colisee-builder",
  "version": "0.1.1",
  "description": "",
  "main": "src/app",
  "scripts": {
    "start": "node src/app.js",
    "start-prod": "NODE_ENV=production node src/app.js",
    "start-dev": "npm run build && node src/app.js",
    "setup": "npm install",
    "test": "npm run build && mocha test/unit test/integration",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "clean": "npm run clean:src && npm run clean:test && npm run clean:misc",
    "clean:src": "rimraf src/**/*.js src/*.js",
    "clean:test": "rimraf test/**/*.js test/*.js",
    "clean:misc": "rimraf dist/ node_modules/",
    "lint": "tslint --format verbose --project ./tsconfig.json",
    "lint-fix": "tslint --fix --format verbose --project ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/colisee-builder.git"
  },
  "author": "siggame@mst.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/colisee-builder/issues"
  },
  "homepage": "https://github.com/siggame/colisee-builder#readme",
  "devDependencies": {
    "@siggame/colisee-tslint": "^1.0.3",
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.1",
    "@types/express": "^4.0.37",
    "@types/http-errors": "^1.6.1",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.42",
    "@types/multer": "^1.3.3",
    "@types/node": "^8.0.25",
    "@types/winston": "^2.3.5",
    "chai": "^4.1.2",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "http-errors": "^1.6.2",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "winston": "^2.3.1"
  }
}
